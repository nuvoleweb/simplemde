{
  "name": "drupal/simplemde",
  "description": "Integrates with Easy Markdown Editor",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "support": {
    "issues": "https://drupal.org/project/issues/simplemde",
    "source": "https://drupal.org/project/simplemde"
  },
  "keywords": [
    "Drupal",
    "SimpleMDE",
    "EasyMDE"
  ],
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "ionaru/easy-markdown-editor",
        "version": "2.15.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://registry.npmjs.org/easymde/-/easymde-2.15.0.tgz",
          "type": "tar"
        }
      }
    }
  ],
  "require": {
    "ionaru/easy-markdown-editor": "2.15.0"
  },
  "autoload": {
    "psr-4": {
        "Drupal\\simplemde\\": "src/"
    }
  }
}
